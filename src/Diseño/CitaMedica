<script type="text/javascript">
// 	Metodo que actualiza captcha (no funciono al incluirla en utilidades.js por eso se dejo aca)
function reloadCaptcha(idioma) {
// Get GoogleCaptcha iframe
var iframeGoogleCaptcha = $('#divRecaptcha').find('iframe');

if(typeof  iframeGoogleCaptcha[0] !== "undefined"){
	// For setting new language 
	iframeGoogleCaptcha.attr("src", iframeGoogleCaptcha.attr("src").replace(/hl=(.*?)&/, 'hl='+ idioma +'&'));
}
	
}
</script><div id="panelIdioma" class="ui-outputpanel ui-widget"></div>
