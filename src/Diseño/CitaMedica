	<script type="text/javascript">
function datesDisabled(date) {

var holidays = new Array();
var fecha_txt = jQuery.datepicker.formatDate('dd/mm/yy', date);

 // day is from [0 - 6] - [Sunday - Monday]
var day = date.getDay();
if(day==0 || day==6){
	return [ false, ''];
}

if(holidays != null){
	var resp = holidays.indexOf(fecha_txt);	    	
	if(resp == -1){
		 return [ true, ''];
	}else{
		 return [ false, ''];
	}	    	
}    

};

reloadRecaptcha();
</script>

